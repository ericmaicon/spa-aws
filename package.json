{
  "name": "spa-aws",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "postinstall": "find ./node_modules/ -name '.babelrc' -exec rm {} \\;",
    "start": "NODE_ENV=development parcel -p 8080 --no-cache src/index.html",
    "prod": "NODE_ENV=production parcel build src/index.html --public-url /",
    "eslint": "./node_modules/.bin/eslint .",
    "test": "NODE_ENV=test jest --verbose"
  },
  "dependencies": {
    "antd": "^3.2.3",
    "babel-polyfill": "^6.26.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-form": "^7.3.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.2",
    "eslint-plugin-react": "^7.7.0",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.6.2"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/tests/assetsTransformer.js"
    },
    "setupFiles": [
      "<rootDir>/tests/bootstrap.js"
    ]
  }
}
